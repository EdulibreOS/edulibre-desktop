# Gambas Form File 3.0

{ Form Form
  MoveScaled(0,0,66,15)
  Foreground = &HE0E0E0&
  Picture = Picture["img/img_back.png"]
  Border = False
  Stacking = Window.Above
  SkipTaskbar = True
  Margin = True
  Padding = 2
  { panIcon HBox
    MoveScaled(1,0,64,15)
    Spacing = True
    Margin = True
    { picIcon PictureBox
      MoveScaled(1,1,10,8)
      Picture = Picture["icon:/64/file"]
      AutoResize = True
    }
    { Panel1 Panel
      MoveScaled(13,0,50,14)
      Expand = True
      Arrangement = Arrange.Row
      Margin = True
      Padding = 2
      { lblName TextLabel
        MoveScaled(3,1,26,4)
        Font = Font["+2"]
        Expand = True
      }
      { lblext Label
        MoveScaled(3,5,9,3)
        Expand = True
        Padding = 2
        Alignment = Align.TopNormal
      }
      { lblcomment Label
        MoveScaled(3,8,9,3)
        Expand = True
        Alignment = Align.TopNormal
      }
      { lblsize Label
        MoveScaled(3,11,9,3)
        Expand = True
        Alignment = Align.TopNormal
      }
    }
  }
}
