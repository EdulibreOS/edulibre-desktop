Public Sub Form_Open()
  
  'CButtonStart.Image = Picture["icon:/medium/battery"].Image
  CButtonStart.Image = Picture["/usr/share/icons/Numix-Circle/scalable/apps/adobe-air.svg"].Image.Stretch(108, 108)
  
End

Public Sub CButtonStart_Leave()
  
  CButtonStart.H = CButtonStart.H - 22
  CButtonStart.w = CButtonStart.H - 22
  CButtonStart.Image = Picture["/usr/share/icons/Numix-Circle/scalable/apps/adobe-air.svg"].Image
  
End

Public Sub CButtonStart_Enter()
  
  CButtonStart.H = CButtonStart.H + 22
  CButtonStart.w = CButtonStart.H + 22
  CButtonStart.Image = Picture["/usr/share/icons/Numix-Circle/scalable/apps/adobe-air.svg"].Image
  
End
