# Gambas Form File 3.0

{ Form Form
  MoveScaled(0,0,84,56)
  Tracking = True
  Text = ("Select Style Launcher - Innova")
  Icon = Picture["icon:/small/image"]
  Persistent = True
  Resizable = False
  Border = False
  Stacking = Window.Above
  Sticky = True
  Arrangement = Arrange.Vertical
  Spacing = True
  Margin = True
  Padding = 2
  { hzoStyle1 HBox
    MoveScaled(7,0,65,4)
    Spacing = True
    Indent = True
    { lbltitle2 Label
      MoveScaled(4,0,45,4)
      Font = Font["+3"]
      Background = &HFFFFFF&
      Expand = True
      Text = ("Style Launcher")
      Alignment = Align.Center
      Border = Border.Plain
    }
    { picClose PictureBox
      MoveScaled(58,0,4,4)
      Mouse = Mouse.Pointing
      ToolTip = ("Cerrar Ventana")
      Picture = Picture["img/cnb/close.png"]
      Stretch = True
    }
  }
  { ListViewImage ListView
    MoveScaled(0,5,57,46)
    Mouse = Mouse.Pointing
    ToolTip = ("Press double click to apply the changes")
  }
  { HBox5 HBox
    MoveScaled(0,51,66,4)
    Spacing = True
    Indent = True
    { Panel24 Panel
      MoveScaled(1,0,29,4)
      Expand = True
      Arrangement = Arrange.Vertical
    }
    { BtnClose ToolButton
      MoveScaled(50,0,16,4)
      Mouse = Mouse.Pointing
      Text = Shortcut(("Close"), "C")
      Picture = Picture["icon:/small/cancel"]
      Border = True
    }
  }
}
